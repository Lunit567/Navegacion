<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegation"
    app:startDestination="@id/frank_1">

    <fragment
        android:id="@+id/frank_1"
        android:name="luna.martin.navegacion.frank_1"
        android:label="fragment_frank_1"
        tools:layout="@layout/fragment_frank_1" >
        <action
            android:id="@+id/action_frank_1_to_frank_2"
            app:destination="@id/frank_2" />
    </fragment>
    <fragment
        android:id="@+id/frank_2"
        android:name="luna.martin.navegacion.frank_2"
        android:label="fragment_frank_2"
        tools:layout="@layout/fragment_frank_2" >
        <action
            android:id="@+id/action_frank_2_to_frank_1"
            app:destination="@id/frank_1" />
    </fragment>
</navigation>